    "\n",
    "            if content:\n",
    "                print(f\"File {item} read successfully.\")\n",
    "\n",
    "                # Analisar o documento usando o serviço de Análise de Documentos da Azure\n",
    "                poller = document_analysis_client.begin_analyze_document(\n",
    "                    model_id=MODEL_ID,\n",
    "                    document=content\n",
    "                )\n",
    "                result = poller.result()\n",
    "\n",
    "                # Processar o resultado conforme necessário\n",
    "                print(f\"Analysis completed for file {item}.\")\n",
    "\n",
    "                # Iterar sobre as tabelas identificadas no resultado da análise\n",
    "                for table_index, table in enumerate(result.tables):\n",
    "                    # Determinar o número de linhas e colunas na tabela\n",
    "                    num_rows = max(cell.row_index for cell in table.cells) + 1\n",
    "                    num_cols = max(cell.column_index for cell in table.cells) + 1\n",
    "                    \n",
    "                    # Inicializar uma matriz vazia para armazenar os dados da tabela\n",
    "                    table_data = [[\"\" for _ in range(num_cols)] for _ in range(num_rows)]\n",
    "                    \n",
    "                    # Preencher a matriz com os dados extraídos de cada célula da tabela\n",
    "                    for cell in table.cells:\n",
    "                        table_data[cell.row_index][cell.column_index] = cell.content\n",
    "                    \n",
    "                    # Converter a matriz em um DataFrame do pandas\n",
    "                    df = pd.DataFrame(table_data)\n",
    "                    \n",
    "                    # Armazenar o DataFrame no dicionário, usando o nome do arquivo e o índice da tabela como chave\n",
    "                    all_tables[f\"{item}_table_{table_index}\"] = df\n",
    "                    \n",
    "                    # Adicionar informações da tabela à lista\n",
    "                    table_info.append({\n",
    "                        \"file_name\": item,\n",
    "                        \"table_index\": table_index,\n",
    "                        \"num_rows\": num_rows,\n",
    "                        \"num_cols\": num_cols,\n",
    "                        \"table_data\": df  # Adicionando o DataFrame da tabela\n",
    "                    })\n",
    "\n",
