    }
   ],
   "source": [
    "import os\n",
    "import pdfplumber\n",
    "import pandas as pd\n",
    "\n",
    "# Caminho para a pasta contendo os arquivos PDF\n",
    "input_path = r\"C:\\Users\\B28166\\Desktop\\Relatorios_avaliacao\"\n",
    "\n",
    "# Criar uma lista para armazenar todos os DataFrames\n",
    "all_dataframes = []\n",
    "\n",
    "# Iterar sobre cada item na pasta\n",
    "for item in os.listdir(input_path):\n",
    "    item_path = os.path.join(input_path, item)\n",
    "    \n",
    "    # Verificar se o item é um ficheiro PDF\n",
    "    if os.path.isfile(item_path) and item.lower().endswith('.pdf'):\n",
    "        try:\n",
    "            print(f\"Processando arquivo: {item}\")\n",
    "\n",
    "            # Abrir o PDF com pdfplumber\n",
    "            with pdfplumber.open(item_path) as pdf:\n",
    "                # Iterar sobre todas as páginas do PDF\n",
    "                for page_num in range(len(pdf.pages)):\n",
    "                    page = pdf.pages[page_num]\n",
    "                    # Extrair tabelas da página\n",
    "                    tables = page.extract_tables()\n",
    "\n",
    "                    # Converter as tabelas extraídas em DataFrames e armazená-las\n",
    "                    for i, table in enumerate(tables):\n",
    "                        df = pd.DataFrame(table[1:], columns=table[0])  # Usar a primeira linha como cabeçalho\n",
    "                        all_dataframes.append(df)  # Adicionar o DataFrame à lista\n",
    "\n",
    "                        # Exibir o DataFrame\n",
    "                        print(f\"Tabela {i + 1} na página {page_num + 1} do arquivo {item}:\")\n",
    "                        print(df)\n",
    "                        print(\"\\n\" + \"=\"*50 + \"\\n\")\n",
    "        \n",
    "        except Exception as e:\n",
    "            print(f\"Ocorreu um erro ao processar o arquivo {item}: {e}\")\n",
    "\n",
    "# Para visualizar todos os DataFrames coletados\n",
    "if all_dataframes:\n",
    "    # Concatenar todos os DataFrames em um único DataFrame\n",
    "    final_dataframe = pd.concat(all_dataframes, ignore_index=True)\n",
    "    print(\"DataFrame final com todas as tabelas extraídas:\")\n",
    "    print(final_dataframe)\n",
    "else:\n",
    "    print(\"Nenhuma tabela foi extraída.\")"
   ]
  },
