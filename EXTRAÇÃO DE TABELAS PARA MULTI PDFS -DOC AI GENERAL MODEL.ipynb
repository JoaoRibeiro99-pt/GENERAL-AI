    "def extract_table_data(table):\n",
    "    \"\"\"Extrai os dados de uma tabela e os armazena em um DataFrame.\"\"\"\n",
    "    num_rows = max(cell.row_index for cell in table.cells) + 1\n",
    "    num_cols = max(cell.column_index for cell in table.cells) + 1\n",
    "    \n",
    "    # Inicializar uma matriz vazia para armazenar os dados da tabela\n",
    "    table_data = [[\"\" for _ in range(num_cols)] for _ in range(num_rows)]\n",
    "    \n",
    "    # Preencher a matriz com os dados extraídos de cada célula da tabela\n",
    "    for cell in table.cells:\n",
    "        table_data[cell.row_index][cell.column_index] = cell.content\n",
    "    \n",
    "    return pd.DataFrame(table_data)\n",
    "\n",
    "def process_files(input_path):\n",
    "    \"\"\"Processa os arquivos na pasta de entrada e filtra as tabelas desejadas.\"\"\"\n",
    "    filtered_tables = {}\n",
    "    filtered_table_info = []\n",
    "\n",
    "    # Iterar sobre cada item na pasta\n",
    "    for item in os.listdir(input_path):\n",
    "        item_path = os.path.join(input_path, item)\n",
    "        \n",
    "        if os.path.isfile(item_path):\n",
    "            try:\n",
    "                print(f\"Lendo o arquivo {item}...\")\n",
    "                result = analyze_document(item_path)\n",
    "                print(f\"Análise concluída para o arquivo {item}.\")\n",
    "\n",
    "                for table_index, table in enumerate(result.tables):\n",
    "                    df = extract_table_data(table)\n",
    "                    \n",
    "                    # Verificar se a frase desejada está na tabela\n",
    "                    if df.apply(lambda row: row.astype(str).str.contains(\n",
    "                            \"Estimativa do valor de mercado no estado atual\",\n",
    "                            case=False, na=False).any(), axis=1).any():\n",
    "                        \n",
    "                        # Armazenar a tabela filtrada no dicionário com um nome chave único\n",
    "                        filtered_tables[f\"{item}_table_{table_index}\"] = df\n",
    "                        \n",
    "                        # Armazenar as informações sobre a tabela filtrada\n",
    "                        filtered_table_info.append({\n",
    "                            \"file_name\": item,\n",
    "                            \"table_index\": table_index,\n",
    "                            \"num_rows\": df.shape[0],\n",
    "                            \"num_cols\": df.shape[1],\n",
    "                            \"table_data\": df\n",
    "                        })\n",
    "            except Exception as e:\n",
    "                print(f\"Ocorreu um erro ao processar o arquivo {item}: {e}\")\n",
    "\n",
    "    return filtered_tables, filtered_table_info\n",
    "\n",
    "def main(input_path):\n",
    "    filtered_tables, filtered_table_info = process_files(input_path)\n",
    "    \n",
    "    # Criar um DataFrame com informações sobre todas as tabelas filtradas\n",
    "    filtered_info_df = pd.DataFrame(filtered_table_info)\n",
    "    \n",
    "    return filtered_tables, filtered_info_df\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    filtered_tables, filtered_info_df = main(input_path)\n",
    "    \n",
    "    # Exibir as informações das tabelas filtradas\n",
    "    print(filtered_info_df)"
   ]
