    "from PyPDF2 import PdfWriter, PdfReader\n",
    "import io\n",
    "\n",
    "def extract_pages_to_pdf(reader, pages):\n",
    "    pdf_writer = PdfWriter()\n",
    "    for page_num in pages:\n",
    "        pdf_writer.add_page(reader.pages[page_num - 1])  # As páginas são indexadas em 0\n",
    "    output = io.BytesIO()\n",
    "    pdf_writer.write(output)\n",
    "    output.seek(0)\n",
    "    return output\n",
    "\n",
    "# Lista para armazenar os dados extraídos\n",
    "data = []\n",
    "\n",
    "# Iterar sobre os arquivos e processar as páginas identificadas\n",
    "for filename, pages_dict in pdf_page_mapping.items():\n",
    "    pdf_path = os.path.join(input_path, filename)\n",
    "    with open(pdf_path, 'rb') as file:\n",
    "        reader = PdfReader(file)\n",
    "        \n",
    "        for text_key, pages in pages_dict.items():\n",
    "            # Extrair páginas específicas para um novo PDF\n",
    "            extracted_pdf = extract_pages_to_pdf(reader, pages)\n",
    "            \n",
    "            # Enviar o PDF extraído para o Azure Form Recognizer\n",
    "            poller = document_analysis_client.begin_analyze_document(MODEL_ID, extracted_pdf)\n",
    "            result = poller.result()\n",
    "\n",
    "            # Adicionar os pares chave-valor ao DataFrame\n",
    "            for kv_pair in result.key_value_pairs:\n",
    "                key = kv_pair.key.content if kv_pair.key else None\n",
    "                value = kv_pair.value.content if kv_pair.value else None\n",
    "                confidence = kv_pair.confidence if kv_pair.confidence else None\n",
    "                data.append({'document': filename, 'page': pages, 'text_key': text_key, 'key': key, 'value': value, 'confidence': confidence})\n",
    "\n",
