    "texts_to_find = [\"5 - CÁLCULOS DE AVALIAÇÃO\", \"7 - RESUMO VALORES\"]\n",
    "\n",
    "# Função para extrair texto de uma página específica\n",
    "def extract_text_from_page(reader, page_number):\n",
    "    page = reader.pages[page_number]\n",
    "    return page.extract_text()\n",
    "\n",
    "# Função para encontrar páginas com textos específicos\n",
    "def find_pages_with_text(pdf_path, texts):\n",
    "    pages_with_text = {text: [] for text in texts}\n",
    "    with open(pdf_path, 'rb') as file:\n",
    "        reader = PdfReader(file)\n",
    "        num_pages = len(reader.pages)\n",
    "        for page_number in range(num_pages):\n",
    "            text = extract_text_from_page(reader, page_number)\n",
    "            if text:\n",
    "                for text_to_find in texts:\n",
    "                    if text_to_find in text:\n",
    "                        pages_with_text[text_to_find].append(page_number + 1)  # Páginas são baseadas em 1\n",
    "    return pages_with_text\n",
    "\n",
    "# Função para processar todos os PDFs na pasta\n",
    "def process_all_pdfs(folder_path, texts):\n",
    "    pdf_page_mapping = {}\n",
    "    for filename in os.listdir(folder_path):\n",
    "        if filename.lower().endswith('.pdf'):\n",
    "            pdf_path = os.path.join(folder_path, filename)\n",
    "            print(f\"Processing file: {filename}\")\n",
    "            pages_with_text = find_pages_with_text(pdf_path, texts)\n",
    "            pdf_page_mapping[filename] = pages_with_text\n",
    "            # Imprimir páginas encontradas para este arquivo\n",
    "            for text, pages in pages_with_text.items():\n",
    "                print(f\"File '{filename}' - Pages with '{text}': {pages}\")\n",
    "    return pdf_page_mapping\n",
    "\n",
    "# Executar a função para processar todos os PDFs na pasta\n",
    "pdf_page_mapping = process_all_pdfs(input_path, texts_to_find)"
